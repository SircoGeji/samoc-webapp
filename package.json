{
  "name": "samoc-webapp",
  "version": "5.12.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --configuration=dev",
    "build:qa": "ng build --configuration=qa",
    "build:clndev": "ng build --configuration=clndev",
    "build:prod": "ng build --configuration=production",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "e2e": "ng e2e",
    "lint-ts": "eslint 'src/app/**/**/*.*.ts' --fix",
    "lint-style": "stylelint 'src/app/**/**/*.scss' --syntax scss --fix",
    "lint": "yarn lint-style && yarn lint-ts",
    "postinstall": "husky install",
    "cypress:local": "npx cypress run --env ENV_URL=http://localhost:4200,ENV=LOCAL,ENV_SERVER_URL=http://127.0.0.1:1337",
    "cypress:dev": "npx cypress run --browser chrome --env ENV_URL=https://samoc-dev.twlght.com,ENV=DEV,ENV_SERVER_URL=https://samoc-dev-server.twlght.com",
    "cypress:qa": "npx cypress run --env ENV_URL=https://samoc-qa.twlght.com,ENV=QA,ENV_SERVER_URL=https://samoc-qa-server.twlght.com",
    "cypress:prod": "npx cypress run --env ENV_URL=https://samoc.twlght.com,ENV=PROD,ENV_SERVER_URL=https://samoc-server.twlght.com",
    "report:allure": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "test:allure-dev": "npm run cypress:dev && npm run report:allure"
  },
  "lint-staged": {
    "cypress/**/*.{js}": [
      "prettier --write",
      "eslint --quiet --fix"
    ],
    "src/app/**/*.ts": [
      "prettier --write",
      "eslint --quiet --fix"
    ],
    "src/app/**/*.js": [
      "prettier --write",
      "eslint --quiet --fix"
    ],
    "src/app/**/*.json": [
      "prettier --write",
      "eslint --quiet --fix"
    ],
    "src/**/*.scss": [
      "stylelint --syntax scss --fix"
    ],
    "**/*.md": [
      "prettier --write"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.3",
    "@angular/cdk": "^11.2.2",
    "@angular/common": "~11.2.3",
    "@angular/compiler": "~11.2.3",
    "@angular/core": "~11.2.3",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "~11.2.3",
    "@angular/material": "^11.2.2",
    "@angular/material-moment-adapter": "^11.2.2",
    "@angular/platform-browser": "~11.2.3",
    "@angular/platform-browser-dynamic": "~11.2.3",
    "@angular/router": "~11.2.3",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "cypress-real-events": "^1.7.0",
    "date-fns": "^2.17.0",
    "docker": "^1.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.4.0",
    "file-saver": "^2.0.5",
    "find-array-duplicates": "^1.0.6",
    "highlight.js": "^10.7.3",
    "jsdoctypeparser": "^9.0.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.23",
    "karma-coverage": "^2.0.3",
    "lint-staged": "^10.5.4",
    "luxon": "^3.4.4",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "mustache": "^4.2.0",
    "ngx-logger": "^4.2.1",
    "ngx-material-timepicker": "^5.5.3",
    "pluralize": "^8.0.0",
    "rxjs": "~6.6.0",
    "socket.io-client": "^3.1.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "tslib": "^2.0.0",
    "vanilla-text-mask": "^5.1.1",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "11.0.0",
    "@angular-devkit/build-angular": "~0.1102.2",
    "@angular/cli": "~11.2.2",
    "@angular/compiler-cli": "~11.2.3",
    "@shelex/cypress-allure-plugin": "^2.38.3",
    "@types/hashids": "^1.0.32",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/mustache": "^4.1.2",
    "@types/node": "^12.11.1",
